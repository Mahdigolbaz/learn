_uimg()
{
    if ! command -v "uimg" >/dev/null 2>&1; then
        return
    fi

    #defining local vars
    local cur prev words cword
    _init_completion || return

    #cur="${COMP_WORDS[COMP_CWORD]}"
    #prev="${COMP_WORDS[COMP_CWORD-1]}"

    #general options
    case "${cur}" in
        --*) return ;;
         -*) COMPREPLY="-h"; return ;;
          *) _filedir; return ;;
    esac
} &&
complete -F _uimg uimg

