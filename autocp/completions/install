_installcustom() {
    if ! command -v "install" >/dev/null 2>&1; then
        return
    fi

    #defining local vars
    local cur prev words cword
    _init_completion || return

    case "${cur}" in
    *) COMPREPLY=($(apt-cache --no-generate pkgnames "${cur}" 2> /dev/null))
       return 0
       ;;
    esac
} && complete -F _installcustom install

